    "templates":[
        {   "parameters": ["DEPARTMENTGROUP_PARAMETER", "TIMEFRAME_PARAMETER"]
        ,   "desc":"Total sales in DEPARTMENTGROUP_PARAMETER TIMEFRAME_PARAMETER."
        ,   "questions":["What were my total sales in DEPARTMENTGROUP_PARAMETER TIMEFRAME_PARAMETER?"]
        ,   "query":"SELECT CONCAT('The total sales in the ', B.TIMEFRAME_DESC ,' timeframe for the ', A.SALES_DEPARTMENT_GROUP_DISPLAY_LABEL,' department is $', LTRIM(TO_VARCHAR(SUM(OP.NET_SALES_DOLLARS_TY), '999,999,999,990')) ) FROM PC.\"Opportunity_Locator\" OP JOIN PC.LKP_SALES_DEPARTMENT_GROUP A ON OP.SALES_DEPARTMENT_GROUP_ID = A.SALES_DEPARTMENT_GROUP_ID JOIN PC.LKP_TIMEFRAME B ON OP.TIMEFRAME_ID = B.TIMEFRAME_ID WHERE A.SALES_DEPARTMENT_GROUP_ID = DEPARTMENTGROUP_PARAMETER AND B.TIMEFRAME_ID = TIMEFRAME_PARAMETER AND OP.USED_IN_METRICS = TRUE GROUP BY A.SALES_DEPARTMENT_GROUP_DISPLAY_LABEL, B.TIMEFRAME_DESC;"
        ,   "urlpage":"opportunity-locator"
        ,   "urlfilter":"{\"departmentDrivers\":[\"DEPARTMENTGROUP_PARAMETER\"]}"
        ,   "urlquery":"{\"timeframe\":TIMEFRAME_PARAMETER}"
        ,   "urlview":"{\"tab::timeline\":\"departmentDrivers\"}"
        },
        {   "parameters": ["EXCLUDE_DEPARTMENTGROUP_PARAMETER", "TIMEFRAME_PARAMETER"]
        ,   "desc":"Total sales excluding EXCLUDE_DEPARTMENTGROUP_PARAMETER TIMEFRAME_PARAMETER."
        ,   "questions":["What were my total sales excluding EXCLUDE_DEPARTMENTGROUP_PARAMETER TIMEFRAME_PARAMETER?", "What were my total sales not including EXCLUDE_DEPARTMENTGROUP_PARAMETER TIMEFRAME_PARAMETER?"]
        ,   "query":"SELECT CONCAT('The total sales in the ', B.TIMEFRAME_DESC ,' timeframe excluding the ', A.SALES_DEPARTMENT_GROUP_DISPLAY_LABEL,' department is $', LTRIM(TO_VARCHAR(SUM(OP.NET_SALES_DOLLARS_TY), '999,999,999,990')) ) FROM PC.\"Opportunity_Locator\" OP JOIN PC.LKP_SALES_DEPARTMENT_GROUP A ON A.SALES_DEPARTMENT_GROUP_ID NOT IN (EXCLUDE_DEPARTMENTGROUP_PARAMETER) AND A.SALES_DEPARTMENT_GROUP_ID <> -1 JOIN PC.LKP_TIMEFRAME B ON OP.TIMEFRAME_ID = B.TIMEFRAME_ID WHERE OP.SALES_DEPARTMENT_GROUP_ID IN (EXCLUDE_DEPARTMENTGROUP_PARAMETER) AND OP.TIMEFRAME_ID = TIMEFRAME_PARAMETER AND OP.USED_IN_METRICS = TRUE GROUP BY A.SALES_DEPARTMENT_GROUP_DISPLAY_LABEL, B.TIMEFRAME_DESC;"
        ,   "urlpage":"opportunity-locator"
        ,   "urlfilter":"{\"departmentDrivers\":[EXCLUDE_DEPARTMENTGROUP_PARAMETER]}"
        ,   "urlquery":"{\"timeframe\":TIMEFRAME_PARAMETER}"
        ,   "urlview":"{\"tab::timeline\":\"departmentDrivers\"}"
        },
        {   "parameters": ["TIMEFRAME_PARAMETER"]
        ,   "desc":"The top performing store TIMEFRAME_PARAMETER."
        ,   "questions":["What is my top performing store TIMEFRAME_PARAMETER?"]
        ,   "query":"SELECT TOP 1 CONCAT(A.STORE_LABEL, ' is the top performing store in the ', B.TIMEFRAME_DESC ,' timeframe with $', LTRIM(TO_VARCHAR(SUM(OP.NET_SALES_DOLLARS_TY), '999,999,999,990')), ' of sales.' ) FROM PC.\"Opportunity_Locator\" OP JOIN PC.LKP_STORE A ON OP.STORE_ID = A.STORE_ID JOIN PC.LKP_TIMEFRAME B ON OP.TIMEFRAME_ID = B.TIMEFRAME_ID WHERE B.TIMEFRAME_ID = TIMEFRAME_PARAMETER AND OP.USED_IN_METRICS = TRUE AND STORE_CONCEPT_ID = 1 GROUP BY A.STORE_LABEL, B.TIMEFRAME_DESC ORDER BY SUM(OP.NET_SALES_DOLLARS_TY) DESC;"
        ,   "urlpage":"opportunity-locator"
        ,   "urlfilter":""
        ,   "urlquery":"{\"timeframe\":TIMEFRAME_PARAMETER}"
