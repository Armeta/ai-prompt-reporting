        ,   "query":"SELECT CONCAT('The total sales last week excluding the ', A.SALES_DEPARTMENT_GROUP_DISPLAY_LABEL,' department is $', LTRIM(TO_VARCHAR(SUM(OP.NET_SALES_DOLLARS_TY), '999,999,999,990')) ) FROM PC.\"Opportunity_Locator\" OP JOIN PC.LKP_SALES_DEPARTMENT_GROUP A ON A.SALES_DEPARTMENT_GROUP_ID NOT IN (EXCLUDE_DEPARTMENTGROUP_PARAMETER) AND A.SALES_DEPARTMENT_GROUP_ID <> -1 WHERE OP.SALES_DEPARTMENT_GROUP_ID IN (EXCLUDE_DEPARTMENTGROUP_PARAMETER) AND OP.TIMEFRAME_ID = 10 AND OP.USED_IN_METRICS = TRUE GROUP BY A.SALES_DEPARTMENT_GROUP_DISPLAY_LABEL;"
